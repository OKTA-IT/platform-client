{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "rules": {
    "align": [true, "statements", "parameters"],
    "arrow-parens": [true, "ban-single-arg-parens"],
    "array-type": [true, "array"],
    "ban": [false],
    "callable-types": true,
    "class-name": true,
    "comment-format": [true, "check-space"],
    "curly": true,
    "trailing-comma": [false],
    "ordered-imports": false,
    "eofline": true,
    "import-spacing": true,
    "indent": [true, "spaces", 2],
    "interface-name": [true, "always-prefix"],
    "interface-over-type-literal": true,
    "jsdoc-format": true,
    "linebreak-style": [true, "CRLF"],
    "max-line-length": [false, 140],
    "member-access": false,
    "member-ordering": [
      false,
      {
        "order": "fields-first"
      }
    ],
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-console": [true, "debug", "info", "time", "timeEnd", "trace"],
    "no-construct": true,
    "no-magic-numbers": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": false,
    "no-empty-interface": true,
    "no-eval": true,
    "no-inferrable-types": [false],
    "no-shadowed-variable": true,
    "no-string-literal": false,
    "no-unused-variable": true,
    "no-string-throw": true,
    "no-var-requires": false,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": true,
    "no-unused-expression": false,
    "no-use-before-declare": false,
    "no-var-keyword": true,
    "object-literal-key-quotes": false,
    "object-literal-sort-keys": false,
    "one-line": [true, "check-open-brace", "check-catch", "check-else", "check-finally", "check-whitespace"],
    "one-variable-per-declaration": [false, "ignore-for-loop"],
    "prefer-const": true,
    "quotemark": [true, "single"],
    "radix": true,
    "semicolon": [true, "always", "ignore-bound-class-methods"],
    "triple-equals": [true, "allow-null-check"],
    "typedef": [true, "parameter", "arrow-parameter", "member-variable-declaration"],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "typeof-compare": true,
    "variable-name": [false, "check-format", "allow-leading-underscore", "ban-keywords"],
    "whitespace": [true, "check-branch", "check-decl", "check-operator", "check-separator", "check-type"]
  }
}
